    "autobind-decorator": "^2.4.0",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "nodemon": "^1.18.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.0.1"
  }
}
